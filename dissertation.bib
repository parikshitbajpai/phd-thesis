	journal = {IEEJ Transactions on Electrical and Electronic Engineering},
	keywords = {swarm intelligence, metaheuristics, global optimization, particle swarm optimization, numerical stability analysis, parameter adjustment},
	number = {6},
	pages = {642-659},
	title = {Particle swarm optimization: a numerical stability analysis and parameter adjustment based on swarm activity},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/tee.20326},
	volume = {3},
	year = {2008},
