	journal = {Nuclear Engineering and Design},
	keywords = {Molten salt reactor, Gibbs energy minimization, System model, TRANSFORM, Thermochimica, Mass accountancy},
	pages = {111695},
	title = {Dynamic mass accountancy modeling of a molten salt reactor using equilibrium thermodynamics},
