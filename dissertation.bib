	isbn = {978-0-08-102866-7},
	keywords = {Cladding, Nuclear fuel, Pellet clad interaction, Stress corrosion cracking, Zirconium},
	pages = {248-306},
	publisher = {Elsevier},
	title = {Pellet-Clad Interaction Behavior in Zirconium Alloy Fuel Cladding},
	url = {https://www.sciencedirect.com/science/article/pii/B978012803581809799X},
