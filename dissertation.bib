	journal = {Computers \& Chemical Engineering},
	keywords = {multiphase equilibrium calculations, equation-solving methods},
	number = {7},
	pages = {897 - 911},
	title = {A method for multiphase equilibrium calculations},
	url = {http://www.sciencedirect.com/science/article/pii/S0098135497002238},
