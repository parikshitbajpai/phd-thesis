@article{INGRI19671261,
	abstract = {A computer program, HALTAFALL, is described which calculates the equilibrium concentrations of the species in mixtures of any number of components, which can form any number of complexes and solid phases, provided concentrations (or partial pressures) can be used in the equilibrium calculations. Of course, the equilibrium constants must be known and enough data be given about the gross composition. Some applications are indicated. The flexible input rules make it applicable to a variety of problems. From the input information, the program itself devises an efficient plan for solving the simultaneous equations. The program also provides an efficient means of finding out which of many possible solid phases can actually appear in a certain equilibrium mixture.
Zusammenfassung
Das Rechenprogramm HALTAFALL wird beschrieben, das die Gleichgewichtskonzentrationen der Spezies in Gemischen aus jeder Zahl von Komponenten berechnet, die jede Anzahl von Komplexen und festen Reaktionsprodukten bilden k{\"o}nnen, sofern bei den Gleichgewichtsberechnungen Konzentrationen (oder Partialdrucke) benutzt werden k{\"o}nnen. Selbstverst{\"a}ndlich m{\"u}ssen die Gleichgewichtskonstanten und gen{\"u}gend Daten {\"u}ber die analytische Zusammensetzung bekannt sein. Auf einige Anwendungen wird hingewiesen. Die flexiblen Eingangsregeln machen seine Anwendung auf ein breites Spektrum von Fragestellungen m{\"o}glich. Aus der eingegebenen Information ermittelt das Programm selbst einen wirksamen Plan zur L{\"o}sung der simultanen Gleichungen. Das Programm enth{\"a}lt auch die M{\"o}glichkeit, herauszufinden, welche unter vielen m{\"o}glichen festen Phasen in einer gegebenen Gleichgewichtsmischung wirklich erscheint.
R{\'e}sum{\'e}
On d{\'e}crit un programme de computation, HALTAFALL, qui calcule les concentrations {\`a} l'{\'e}quilibre des esp{\`e}ces dans des m{\'e}langes {\`a} n'importe quel nombre de composants, qui peuvent former n'importe quel nombre de complexes et de phases solides, en admettant qu'on peut utiliser les concentrations (ou les pressions partielles) dans les calculs d'{\'e}quilibre. Evidemment, les constantes d'{\'e}quilibre doivent {\^e}tre connues et l'on doit fournir suffisamment de donn{\'e}es sur la composition brute. On indique quelques applications. Les r{\`e}gles souples d'admission le rendent applicable {\`a} des probl{\`e}mes vari{\'e}s. A partir de l'information de d{\'e}part, le programme lui-m{\^e}me {\'e}tablit un plan convenable pour la r{\'e}solution des {\'e}quations simultan{\'e}es. Le programme fournit aussi un moyen efficace pour trouver laquelle des nombreuses phases solides possibles peut r{\'e}ellement appara{\^\i}tre dans un certain m{\'e}lange en {\'e}quilibre.},
	author = {Nils Ingri and Wladys{\l}aw Kakolowicz and Lars Gunnar Sill{\'e}n and Bj{\"o}rn Warnqvist},
	date-added = {2019-11-11 15:59:24 -0500},
	date-modified = {2019-11-11 16:00:41 -0500},
	doi = {https://doi.org/10.1016/0039-9140(67)80203-0},
	issn = {0039-9140},
	journal = {Talanta},
	number = {11},
	pages = {1261 - 1286},
	title = {High-speed computers as a supplement to graphical methods -- {V:} {H}altafall, a general program for calculating the composition of equilibrium mixtures},
	url = {http://www.sciencedirect.com/science/article/pii/0039914067802030},
	volume = {14},
	year = {1967},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/0039914067802030},
	bdsk-url-2 = {https://doi.org/10.1016/0039-9140(67)80203-0}}

@article{HAN1998897,
	abstract = {Methods for multiphase equilibrium calculations are generally based on Gibbs free energy minimization or equation-solving. Equation-solving methods, although superior to free energy minimization methods for phase equilibrium calculations without chemical reactions, often involve sequential procedures for a priori phase identification. A simultaneous equation-solving method (τ-method), based on modifying mole fraction summations, is proposed for these calculations. It requires the solution of a minimization problem only once, and provides phases actually present at equilibrium, their quantities and compositions simultaneously; and phase identification in advance is not required. Phase characteristic variable and pseudo phase are introduced, and their significance discussed. τ-method is shown by analysis and numerical results, to be consistent with Nelson's (1987, Comput. Chem. Engng 11, 581--591) criteria for phase existence. The method is tested on typical examples for two-phase (vapor--liquid and liquid--liquid) and three-phase (vapor--liquid--liquid) equilibrium calculations. For each example, several conditions and/or different initializations are used, and the minimization problem is solved by a version of successive linear programming method. The results show that τ-method is successful and reliable for multiphase equilibrium calculations.},
	author = {Guangjun Han and Gade Pandu Rangaiah},
	date-added = {2019-11-11 15:56:18 -0500},
	date-modified = {2019-11-11 16:23:32 -0500},
	doi = {https://doi.org/10.1016/S0098-1354(97)00223-8},
	issn = {0098-1354},
	journal = {Computers \& Chemical Engineering},
	keywords = {multiphase equilibrium calculations, equation-solving methods},
	number = {7},
	pages = {897 - 911},
	title = {A method for multiphase equilibrium calculations},
	url = {http://www.sciencedirect.com/science/article/pii/S0098135497002238},
	volume = {22},
	year = {1998},
	bdsk-url-1 = {http://www.sciencedirect.com/science/article/pii/S0098135497002238},
	bdsk-url-2 = {https://doi.org/10.1016/S0098-1354(97)00223-8}}

@article{PENGLEE19991183,
	abstract = {Direct search optimization is applied to Gibbs free energy minimization to determine phase compositions at equilibrium. The method selected is the random search optimization procedure of Luus and Jaakola, which has been shown to be successful for solving difficult global optimization problems. It is implemented in a multipass fashion where the region size for a variable at the beginning of each pass is put equal to the change in that variable in the previous pass. Results obtained with typical examples for vapor--liquid equilibrium and vapor--liquid equilibrium with reaction, where the liquid and vapor phases may be non-ideal, show that the LJ optimization procedure can be used to find very accurately the global minimum in only a few seconds of computation time on a personal computer. The method is easy to use and has high reliability of locating the global optimum, thus providing a useful means of checking the results obtained by other methods.},
