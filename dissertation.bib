	issn = {1570-7946},
	keywords = {Global optimization, Gibbs minimization, multiphase equilibrium},
	pages = {585 - 590},
	publisher = {Elsevier},
	series = {Computer Aided Chemical Engineering},
	title = {Reliability vs. Efficiency When Solving Multiphase Equilibrium Problems with Hybrid Optimization Codes},
