	journal = {Journal of Nuclear Materials},
	keywords = {Molten salts, Ni-based alloys, Corrosion rates, Coupled thermodynamic-kinetic modeling},
	pages = {152755},
	title = {First steps toward predicting corrosion behavior of structural materials in molten salts},
